    "@typescript-eslint/parser": "^2.27.0",
    "babel-loader": "^8.1.0",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.10.1",
    "eslint-plugin-prettier": "^3.1.3",
    "eslint-plugin-react": "^7.19.0",
    "husky": "^4.2.5",
    "jest": "^25.3.0",
    "prettier": "2.0.4",
    "ts-jest": "^25.3.1",
    "typescript": "^3.8.3"
